@book{FiberAgrawal,
author={Govind P. Agrawal},
booktitle = {Fiber‐Optic Communication Systems},
publisher = {John Wiley \& Sons, Ltd},
isbn = {9780470918524},
title = {Optical Fibers},
chapter = {2},
pages = {24-78},
doi = {10.1002/9780470918524.ch2},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/9780470918524.ch2},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/9780470918524.ch2},
year = {2011},
keywords = {lightwave system, Maxwell equation, optical fibers},
abstract = {Summary This chapter focuses on the role of optical fibers as a communication channel in lightwave systems. It explains the guiding mechanism and introduces the related basic concepts. Maxwell’s equations are used to describe wave propagation in optical fibers. The chapter then discusses the origin of fiber dispersion. It considers limitations on the bit rate and the transmission distance imposed by fiber dispersion. The chapter then focuses on loss mechanisms in optical fibers and nonlinear effects. Finally it discusses the engineering aspects of optical fibers made using either silica glass or a suitable plastic material. Manufacturing of fiber cables, suitable for use in an actual lightwave system, involves sophisticated technology with attention to many practical details. It begins with silica fibers and then considers plastic fibers. Both types of materials have been used in recent years to make microstructured fibers discussed in a separate subsection. Controlled Vocabulary Terms communication channels; optical fibers; wave propagation}
}

@article{Zibar:12,
author = {Darko Zibar and Ole Winther and Niccolo Franceschi and Robert Borkowski and Antonio Caballero and Valeria Arlunno and Mikkel N. Schmidt and Neil Guerrero Gonzales and Bangning Mao and Yabin Ye and Knud J. Larsen and Idelfonso Tafur Monroy},
journal = {Opt. Express},
keywords = {Fiber optics and optical communications; Coherent communications; Analog to digital converters; Nonlinear optical fibers; Nonlinear optical signal processing; Optical signal to noise ratio; Phase noise; Self phase modulation},
number = {26},
pages = {B181--B196},
publisher = {OSA},
title = {Nonlinear impairment compensation using expectation maximization for dispersion managed and unmanaged PDM 16-QAM transmission},
volume = {20},
month = {Dec},
year = {2012},
url = {http://www.opticsexpress.org/abstract.cfm?URI=oe-20-26-B181},
doi = {10.1364/OE.20.00B181},
abstract = {In this paper, we show numerically and experimentally that expectation maximization (EM) algorithm is a powerful tool in combating system impairments such as fibre nonlinearities, inphase and quadrature (I/Q) modulator imperfections and laser linewidth. The EM algorithm is an iterative algorithm that can be used to compensate for the impairments which have an imprint on a signal constellation, i.e. rotation and distortion of the constellation points. The EM is especially effective for combating non-linear phase noise (NLPN). It is because NLPN severely distorts the signal constellation and this can be tracked by the EM. The gain in the nonlinear system tolerance for the system under consideration is shown to be dependent on the transmission scenario. We show experimentally that for a dispersion managed polarization multiplexed 16-QAM system at 14 Gbaud a gain in the nonlinear system tolerance of up to 3 dB can be obtained. For, a dispersion unmanaged system this gain reduces to 0.5 dB.},
}
